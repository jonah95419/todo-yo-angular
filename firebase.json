{
    "database": {
        "rules": "dist/todo-yo/rules-firebase.json"
    },
    "hosting": {
        "public": "dist/todo-yo",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "rewrites": [{
            "source": "**",
            "destination": "/index.html"
        }]
    },
    "storage": {
        "rules": "dist/todo-yo/storage.rules"
    }
}
